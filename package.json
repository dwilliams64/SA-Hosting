{
  "name": "SAHosting",
  "version": "1.0.0",
  "description": "SAHosting website",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass src/sass/main.scss:src/css/style.css -w",
    "compile:sass": "sass src/sass/main.scss:src/css/style.css",
    "concat:css": "concat -o src/css/style.concat.css src/css/icon-font.css src/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/css/style.concat.css -o src/css/style.prefix.css",
    "compress:css": "node-sass src/css/style.prefix.css src/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "PetriD",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "chokidar": "^3.0.2",
    "concat": "^1.0.3",
    "cssgrace": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^6.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.17",
    "postcss-clip-path-polyfill": "^1.0.4"
  },
  "dependencies": {}
}
